{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "corejs": 3,
        "useBuiltIns": "usage",
        "targets": {
          "ie": "11"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["polyfill-corejs3", {
      "method": "usage-global"
    }],
    "@babel/syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties"
  ]
}
